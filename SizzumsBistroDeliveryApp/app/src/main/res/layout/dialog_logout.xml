<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="240dp"
        android:layout_height="200dp"
        app:cardCornerRadius="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:backgroundTint="#fafafa"
            android:background="@drawable/catecon2">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/rectangle_10_topleft_topright"
                android:backgroundTint="@color/white"
                android:fontFamily="sans-serif-black"
                android:padding="15dp"
                android:textSize="16sp"
                android:text="Logout"
                android:textStyle="normal"
                android:textColor="@color/black"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-medium"
                android:layout_marginEnd="20dp"
                android:layout_marginStart="20dp"
                android:textAlignment="gravity"
                android:gravity="center_horizontal"
                android:justificationMode="inter_word"
                android:text="Are you sure you want\nto logout?"
                app:layout_constraintBottom_toTopOf="@+id/btncancel"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btncancel"
                android:layout_width="115dp"
                android:textAllCaps="false"
                android:layout_height="45dp"
                android:background="@drawable/rectangle_10_bottomleft"
                android:fontFamily="sans-serif-medium"
                android:text="Cancel"
                android:layout_margin="5dp"
                android:backgroundTint="#fdfdfd"
                android:textSize="14sp"
                android:textColor="@color/gray"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView6"
                app:layout_constraintVertical_bias="1" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnconfirm"
                android:layout_width="115dp"
                android:layout_height="45dp"
                android:background="@drawable/rectangle_10_bottomright"
                android:fontFamily="sans-serif-medium"
                android:text="Confirm"
                android:backgroundTint="@color/wall1"
                android:layout_margin="5dp"
                android:textAllCaps="false"
                app:layout_constraintHorizontal_bias="1"
                app:layout_constraintVertical_bias="1"
                android:textColor="@color/white"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/btncancel"
                app:layout_constraintTop_toBottomOf="@+id/textView6" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>